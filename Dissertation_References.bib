journal = {Applied microbiology and biotechnology},
keywords = {Acetic acid ; Bacteria ; Biochemical fuel cells ; Biofilms ; Biomedical and Life Sciences ; Biotechnology ; Cell culture ; Clostridium ; Cold regions ; Cold treatment ; Electric potential ; Electric power generation ; Electric power production ; Electric properties ; Electricity ; Electron transfer ; Enrichment ; Enterococcus ; Environmental Biotechnology ; Fermentation ; Fuel cells ; Fuel technology ; Geobacter ; Life Sciences ; Low temperature ; Methods ; Microbial Genetics and Genomics ; Microbiology ; Microorganisms ; Voltage ; Wastewater treatment},
